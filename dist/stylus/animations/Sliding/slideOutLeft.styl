/* @group Slide Out Left Transition */
@keyframes slideOutLeft
  0%
    transform: translate3d(0, 0, 0)
  100%
    transform: translate3d(-100%, 0, 0)
    visibility: hidden

// Set to apply a slide out left effect to an element.
.slideOutLeft
  animation-name: slideOutLeft
/* @end */
