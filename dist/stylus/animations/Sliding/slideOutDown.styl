/* @group Slide Out Down Transition */
@keyframes slideOutDown
  0%
    transform: translate3d(0, 0, 0)
  100%
    transform: translate3d(0, 100%, 0)
    visibility: hidden

// Set to apply a fade out down effect to an element.
.slideOutDown
  animation-name: slideOutDown
/* @end */
