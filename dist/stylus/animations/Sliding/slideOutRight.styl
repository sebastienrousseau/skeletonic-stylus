/* @group Slide Out Right Transition */
@keyframes slideOutRight
  0%
    transform: translate3d(0, 0, 0)
  100%
    transform: translate3d(100%, 0, 0)
    visibility: hidden

// Set to apply a slide out right effect to an element.
.slideOutRight
  animation-name: slideOutRight
/* @end */
