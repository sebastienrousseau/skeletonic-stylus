/* @group Slide In Right Transition */
@keyframes slideInRight
  0%
    transform: translate3d(100%, 0, 0)
    visibility: visible
  100%
    transform: translate3d(0, 0, 0)

// Set to apply a slide in right effect to an element.
.slideInRight
  animation-name: slideInRight
/* @end */
