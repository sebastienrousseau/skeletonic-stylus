/* @group Vanish Out animation */
@keyframes vanishOut
  0%
    filter: blur(0px)
    opacity: 1
    transform-origin: 50% 50%
    transform: scale(1, 1)
  100%
    filter blur(161.8px)
    opacity: 0
    transform-origin: 50% 50%
    transform: scale(2, 2)

.vanishOut
  animation-name: vanishOut
/* @end */
