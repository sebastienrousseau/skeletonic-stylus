/* @group Fade In Down Transition */
@keyframes fadeInDown
  0%
    opacity: 0
    transform: translateY(-16.18vh)
  100%
    opacity: 1
    transform: translateY(0)

// Set to apply a fade in down effect to an element.
.fadeInDown
  animation-name: fadeInDown
/* @end */
