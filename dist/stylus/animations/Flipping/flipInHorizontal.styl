/* @group Flip Horizontal Animation */
@keyframes flipInHorizontal
  from
    animation-timing-function: ease-in
    opacity: 0
    transform: perspective(1618px) rotate3d(0, 1, 0, 90deg)
  40%
    animation-timing-function: ease-in
    transform: perspective(1618px) rotate3d(0, 1, 0, -20deg)
  60%
    opacity: 1
    transform: perspective(1618px) rotate3d(0, 1, 0, 10deg)
  80%
    transform: perspective(1618px) rotate3d(0, 1, 0, -5deg)
  to
    transform: perspective(1618px)

.flipInHorizontal
  animation-name: flipInHorizontal
/* @end */
