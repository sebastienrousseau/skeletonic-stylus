/* @group Flip Out Vertical Animation */
@keyframes flipOutVertical
  from
    transform: perspective(400px)
  30%
    opacity: 1
    transform: perspective(400px) rotate3d(0, 1, 0, -16.18deg)
  to
    opacity: 0
    transform: perspective(400px) rotate3d(0, 1, 0, 90.16deg)

.flipOutVertical
  animation-name: flipOutVertical
/* @end */
