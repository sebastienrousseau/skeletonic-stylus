/* @group Vanish out animation */
@keyframes vanish-out
  0%
    filter: blur(0px)
    opacity: 1
    transform-origin: 50% 50%
    transform: scale(1, 1)
  100%
    filter blur(100px)
    opacity: 0
    transform-origin: 50% 50%
    transform: scale(2, 2)
.vanish-out
  animation-name: vanish-out 1s
/* @end */
