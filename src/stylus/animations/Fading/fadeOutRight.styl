/* @group Fade Out Right Transition */
@keyframes fadeOutRight
  0%
    opacity: 1
    transform: translateX(16.18vh)
  100%
    opacity: 0
    transform: translateX(0)

// Set to apply a fade out right effect to an element.
.fadeOutRight
  animation-name: fadeOutRight
/* @end */
