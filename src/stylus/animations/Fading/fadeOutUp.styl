/* @group Fade Out Up Transition */
@keyframes fadeOutUp
  0%
    opacity: 1
    transform: translateY(16.18vh)
  100%
    opacity: 0
    transform: translateY(0)

// Set to apply a fade out up effect to an element.
.fadeOutUp
  animation-name: fadeOutUp
/* @end */
