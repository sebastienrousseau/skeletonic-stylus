/* @group Fade Out Left Transition */
@keyframes fadeOutLeft
  0%
    opacity: 1
    transform: translateX(-16.18vh)
  100%
    opacity: 0
    transform: translateX(0)

// Set to apply a fade out left effect to an element.
.fadeOutLeft
  animation-name: fadeOutLeft
/* @end */
