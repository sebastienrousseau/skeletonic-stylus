/* @group Vanish in animation */
@keyframes vanish-in
  0%
    filter: blur(100px)
    opacity: 0
    transform-origin: 50% 50%
    transform: scale(2, 2)
  100%
    filter blur(0px)
    opacity: 1
    transform-origin: 50% 50%
    transform: scale(1, 1)
.vanish-in
  animation-name: vanish-in 1s
/* @end */
