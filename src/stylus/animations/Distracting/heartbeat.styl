/* @group Heartbeat Animation */
@keyframes heartbeat
  0%
    transform scale(.75)
  20%
    transform scale(1)
  40%
    transform scale(.75)
  60%
    transform scale(1)
  80%
    transform scale(.75)
  100%
    transform scale(.75)

.heartbeat
  animation-name: heartbeat
/* @end */
