{
  "name": "skeletonic-stylus",
  "version": "1.2.1",
  "description": "Skeletonic Stylus is a lightweight, intuitive, accessible and ultra-responsive cascading style sheets framework written in Stylus to streamline your Digital and Mobile Web development needs.",
  "keywords": [
    "css",
    "mit-license",
    "open-source",
    "preprocessor",
    "skeletonic-css",
    "skeletonic-stylus",
    "stylus",
    "stylus-css",
    "stylus-mixin"
  ],
  "main": "src/stylus/skeletonic.styl",
  "style": "dist/skeletonic.min.css",
  "unpkg": "dist/skeletonic.css",
  "repository": "git+https://github.com/sebastienrousseau/skeletonic-stylus.git",
  "author": "Sebastien Rousseau (https://github.com/sebastienrousseau/skeletonic-stylus)",
  "license": "MIT",
  "license_URI": "http://www.opensource.org/licenses/mit-license.php",
  "bugs": {
    "url": "https://github.com/sebastienrousseau/skeletonic-stylus.git/issues"
  },
  "homepage": "https://skeletonic.io",
  "scripts": {
    "build": "npm run format && npm run lint && npm run clean && npm run stylus && npm run autoprefix && npm run csso && npm run cssstats",
    "clean": "rimraf dist/css",
    "autoprefix": "postcss -u autoprefixer --no-map.inline --autoprefixer.browsers \"last 1 versions\" --output dist/css/skeletonic.css dist/css/skeletonic.css",
    "csso": "csso -o dist/css/skeletonic.min.css -i dist/css/skeletonic.css --debug --stat",
    "cssstats": "cssstats dist/css/skeletonic.css dist/css/skeletonic.json",
    "cleancss": "cleancss -o dist/css/skeletonic.min.css dist/css/skeletonic.css",
    "format": "stylus-supremacy format ./src/stylus/**/*.styl --options ./.stylelintrc.json",
    "lint": "stylint src/stylus --strict",
    "stylus": "stylus --include-css src/stylus/skeletonic.styl -o dist/css/skeletonic.css"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "autoprefixer-stylus": "^1.0.0",
    "browser-sync": "^2.26.14",
    "clean-css": "^5.1.2",
    "clean-css-cli": "^5.3.0",
    "css-declaration-sorter": "^6.0.3",
    "csslint-stylish": "^0.0.6",
    "cssnano": "^5.0.5",
    "cssnano-preset-advanced": "^5.1.2",
    "csso": "^4.2.0",
    "csso-cli": "^3.0.0",
    "cssstats-cli": "^4.0.2",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-base64": "^0.1.3",
    "gulp-clean-css": "^4.3.0",
    "gulp-combine": "^0.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-concat-css": "^3.1.0",
    "gulp-csscomb": "^3.1.0",
    "gulp-csslint": "^1.0.1",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-gzip": "^1.4.2",
    "gulp-header": "^2.0.9",
    "gulp-if": "^3.0.0",
    "gulp-less": "^4.0.1",
    "gulp-load-plugins": "^2.0.7",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-rev-all": "^3.0.0",
    "gulp-rev-append": "^0.1.8",
    "gulp-rev-napkin": "^0.1.0",
    "gulp-rev-replace": "^0.4.4",
    "gulp-size": "^4.0.1",
    "gulp-sizereport": "^1.2.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-spy": "^1.1.0",
    "gulp-stylelint": "^13.0.0",
    "gulp-stylus": "^2.7.0",
    "gulp-tar": "^3.1.0",
    "gulp-watch": "^5.0.1",
    "gulp4-run-sequence": "^1.0.1",
    "normalize.css": "^8.0.1",
    "postcss": "^8.3.0",
    "postcss-bem-linter": "^3.3.0",
    "postcss-cli": "^8.3.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.5",
    "postcss-preset-env": "^6.7.0",
    "postcss-simple-vars": "^6.0.3",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylus": "^0.54.8",
    "stylus-supremacy": "^2.15.0",
    "watch": "^1.0.2"
  }
}
